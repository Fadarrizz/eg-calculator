{
    "name": "fadarrizz/energyglobal-testcase",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "fadarrizz",
        "energyglobal-testcase"
    ],
    "homepage": "https://github.com/fadarrizz/energyglobal-testcase",
    "license": "MIT",
    "authors": [
        {
            "name": "Auke Geerts",
            "email": "auk.geerts@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "TestCase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestCase\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
