{
    "name": "fadarrizz/eg-calculator",
    "type": "library",
    "keywords": [
        "fadarrizz",
        "eg-calculator"
    ],
    "homepage": "https://github.com/fadarrizz/eg-calculator",
    "license": "MIT",
    "authors": [
        {
            "name": "Auke Geerts",
            "email": "auk.geerts@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0",
        "ext-xmlwriter": "*",
        "ext-readline": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Fadarrizz\\EgCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fadarrizz\\EgCalculator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
